    LocalP.Backpack.ChildAdded:Connect(function(v)
        if v:IsA("Tool") and v.Name == "Katana" then
            v.Grip = CFrame.new(0,-1,-0.16) * CFrame.Angles(math.rad(180),math.rad(0),math.rad(180))
        end
    end)
end)

