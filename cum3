    end
    if Args.Command == "remotecontrol" or Args.Command == "remote" then
        if #Baseplates == 0 then
            for i,v in pairs(game.Workspace:GetChildren()) do
                if string.lower(v.Name) == "baseplate" then -- fuck capital letters
